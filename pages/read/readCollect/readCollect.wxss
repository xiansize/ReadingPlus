

page {
  background: #f5f5f9;
}

.backpress {
  width: 22rpx;
  height: 38rpx;
  position: fixed;
  top: 66rpx;
  left: 32rpx;
  z-index: 100;
}

.wrap {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* margin-top: 128rpx; */
}

.collect{
  width: 100%;
  height: 230rpx;
  background: #FFFFFF;
  border-bottom: solid #DDDDDD 2rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  
}

.collect-item{
  width: 92%;
  height: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.cover{
  width: 18%;
  height: 170rpx;
  border-radius: 6rpx;
  margin-right: 2%;
}

.article{
  width: 78%;
  height: 100%;
}

.collect-icon{
  width: 42rpx;
  height: 42rpx;
}

.article-title{
  width: 86%;
  height: 52%;
  line-height: 170rpx;
  font-size: 30rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.article-author {
  width: 100%;
  height: 48%;
  font-size: 26rpx;
  color: #999;
}



