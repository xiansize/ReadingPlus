}

.nav {
  width: 100%;
  height: 90rpx;
  background: #fff;
  border-bottom: solid #ddd 2rpx;
  display: flex;
  flex-direction: row;
  margin-bottom: 20rpx;
}

.nav-item {
  width: 50%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.nav-name {
  width: 50%;
  height: 90%;
