

page {
  background: #f5f5f9;
}

.backpress {
  width: 22rpx;
  height: 38rpx;
  position: fixed;
  top: 66rpx;
  left: 32rpx;
  z-index: 100;
}

.wrap {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* margin-top: 128rpx; */
}

.read-free {
  width: 300rpx;
  height: 78rpx;
  background: #eee;
  border-radius: 10rpx;
  position: fixed;
  bottom: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.text-read-free {
  line-height: 78rpx;
  text-align: center;
  font-size: 30rpx;
  color: #333;
  margin-right: 12rpx;
}

.icon-read-free {
  width: 20rpx;
  height: 16rpx;
}

.container-category-item {
  width: 690rpx;
  height: 140rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  background: #64acfc;
  border-radius: 20rpx;
  margin: 30rpx 0rpx 0rpx 0rpx;
}

.category-cover {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin: 0rpx 20rpx 0rpx 20rpx;
}

.category-title {
  width: auto;
  color: #fff;
  font-size: 30rpx;
  text-align: center;
}

.container-all-item {
  width: 690rpx;
  height: 62rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-all-item {
  width: 6rpx;
  height: 24rpx;
  border-radius: 3px;
  margin-right: 15rpx;
}

.all-item {
  height: 62rpx;
  line-height: 62rpx;
  color: #999;
  font-size: 24rpx;
}

.container-article {
  width: 690rpx;
  height: 140rpx;
  background: #fff;
  border-radius: 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-bottom: 20rpx;
}

.icon {
  width: 90rpx;
  height: 90rpx;
  margin: 24rpx 26rpx 26rpx 25rpx;
  border-radius: 12px;
}

.container-text {
  width: 476rpx;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.article-title {
  width: 100%;
  height: 80rpx;
  line-height: 100rpx;
  color: #333;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.article-author {
  width: 100%;
  height: 60rpx;
  color: #999;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.to-read {
  width: 44rpx;
  height: 44rpx;
  border-radius: 50%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.icon-to-read {
  width: 23rpx;
  height: 26rpx;
}
