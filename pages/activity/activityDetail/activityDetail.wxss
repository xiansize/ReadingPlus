

page {
  background: #f5f5f9;
  
}

.backpress {
  width: 22rpx;
  height: 38rpx;
  position: fixed;
  top: 66rpx;
  left: 32rpx;
  z-index: 100;
}

.wrap {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  /* margin-top: 128rpx; */
}

.cover {
  width: 100%;
  height: 390rpx;
}

.middle {
  width: 100%;
  height: 216rpx;
  background: #FFFFFF;
  margin-bottom: 20rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-bottom: solid #DDDDDD 2rpx;
}

.activity-title {
  width: 92%;
  color: #222222;
  font-size: 32rpx;
  margin-top: 20rpx;
}

.container-msg {
  width: 92%;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 30rpx;
}

.activity-icon {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}

.activity-msg {
  width: 70%;
  height: 100%;
  color: #999999;
  font-size: 30rpx;
}

.container-summary {
  width: 100%;
  height: auto;
  background: #fff;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.container-summary-title {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.icon-summary {
  width: 30rpx;
  height: 30rpx;
  margin-left: 4%;
  margin-right: 16rpx;
}

.summary-title {
  width: 50%;
  height: 100rpx;
  line-height: 100rpx;
  color: #999;
  font-size: 30rpx;
}

.summary-text {
  width: 92%;
  font-size: 30rpx;
  color: #666;
  line-height: 40rpx;
  letter-spacing: 2rpx;
}


.activity-timeup {
  width: 20%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 24rpx;
  color: red;
  border-radius: 10rpx;
  text-align: center;
}
